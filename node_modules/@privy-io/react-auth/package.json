{
  "version": "1.65.0",
  "name": "@privy-io/react-auth",
  "sideEffects": false,
  "description": "React client for the Privy Auth API",
  "keywords": [
    "authentication",
    "authorization",
    "identity",
    "privacy",
    "privy",
    "user data",
    "web3"
  ],
  "homepage": "https://github.com/orgs/privy-io/discussions",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npx tsup --clean --minify",
    "generate-types": "npx tsup --dts-only",
    "clean": "rm -rf dist",
    "dev": "npx tsup --watch",
    "test": "jest --testMatch \"**/*(?!.*integration).test.ts\"",
    "test-integration": "jest --testMatch \"**/test/integration/**/*.test.ts\"",
    "test:ci": "npm run test",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" && npx tsc --noEmit",
    "format": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix"
  },
  "dependencies": {
    "@coinbase/wallet-sdk": "3.9.3",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/logger": "^5.7.0",
    "@ethersproject/providers": "^5.7.1",
    "@ethersproject/strings": "^5.7.0",
    "@ethersproject/transactions": "^5.7.0",
    "@ethersproject/units": "^5.7.0",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@marsidev/react-turnstile": "^0.4.1",
    "@metamask/eth-sig-util": "^6.0.0",
    "@simplewebauthn/browser": "^9.0.1",
    "@walletconnect/ethereum-provider": "^2.11.1",
    "@walletconnect/modal": "^2.6.2",
    "base64-js": "^1.5.1",
    "dotenv": "^16.0.3",
    "encoding": "^0.1.13",
    "eventemitter3": "^5.0.1",
    "fast-password-entropy": "^1.1.1",
    "jose": "^4.15.5",
    "js-cookie": "^3.0.5",
    "libphonenumber-js": "^1.10.14",
    "lokijs": "^1.5.12",
    "md5": "^2.3.0",
    "mipd": "^0.0.7",
    "ofetch": "^1.3.4",
    "pino-pretty": "^10.0.0",
    "qrcode": "^1.5.1",
    "react-device-detect": "^2.2.2",
    "secure-password-utilities": "^0.2.1",
    "styled-components": "^5.3.6",
    "tinycolor2": "^1.6.0",
    "uuid": ">=8 <10",
    "web3-core": "^1.8.0",
    "web3-core-helpers": "^1.8.0"
  },
  "devDependencies": {
    "@privy-io/eslint-config-custom": "*",
    "@privy-io/public-api": "2.2.0",
    "@saithodev/semantic-release-backmerge": "^2.1.2",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/node16-strictest-esm": "^1.0.3",
    "@types/create-hash": "^1.2.6",
    "@types/fast-password-entropy": "^1.1.1",
    "@types/jest": "^29.1.2",
    "@types/js-cookie": "^3.0.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/md5": "^2.3.2",
    "@types/qrcode": "^1.5.0",
    "@types/randombytes": "^2.0.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.26",
    "@types/tinycolor2": "^1.4.3",
    "@types/uuid": "^9.0.2",
    "jest-environment-jsdom": "^29.7.0",
    "semantic-release": "^19.0.5",
    "ts-jest": "^29.0.3",
    "tsup": "^6.2.3",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "bugs": {
    "url": "https://github.com/orgs/privy-io/discussions"
  },
  "directories": {
    "test": "test"
  },
  "author": "privy.io",
  "browserslist": [
    "defaults",
    "node >= 18",
    "not op_mini all"
  ],
  "publishConfig": {
    "access": "public"
  }
}
